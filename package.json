{
  "name": "cursor-rules",
  "private": true,
  "version": "0.23.1",
  "scripts": {
    "build:coordination": "tsc -p tsconfig.coordination.json",
    "changeset": "changeset",
    "coordination:client": "ts-node src/coordination/cli.ts",
    "coordination:server": "ts-node src/coordination/index.ts",
    "generate:projects-readme": ".cursor/scripts/generate-projects-readme.sh",
    "generate:root-readme": ".cursor/scripts/generate-root-readme.sh",
    "test:coordination:bash": "bash .cursor/scripts/coordination/task-assign.test.sh && bash .cursor/scripts/coordination/report-check.test.sh && bash .cursor/scripts/coordination/task-complete.test.sh",
    "test:coordination": "jest src/coordination",
    "test:scripts": ".cursor/scripts/tests/run.sh -v",
    "validate:ghtoken": "curl -sS -o /dev/null -w \"%{http_code}\\n\" -H \"Authorization: token $GH_TOKEN\" -H \"Accept: application/vnd.github+json\" https://api.github.com",
    "validate:root-readme": ".cursor/scripts/validate-root-readme.sh",
    "version:changesets": "npx -y changeset version && node -e \"const fs=require('fs');const v=require('./package.json').version;fs.writeFileSync('VERSION', v+'\\n');require('child_process').execSync('git add VERSION',{stdio:'inherit'})\""
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@changesets/cli": "^2.29.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/ws": "^8.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  }
}
